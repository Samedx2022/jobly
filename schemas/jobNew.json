{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "salary": {
        "type": "number",
        "minimum": 0
      },
      "equity": {
        "type": "string",
        "pattern": "^(0(?:\\.\\d+)?|1(?:\\.0+)?)$"
      },
      "company_handle": {
        "type": "string"
      }
    },
    "required": ["title", "salary", "equity", "company_handle"],
    "additionalProperties": false
  }
  