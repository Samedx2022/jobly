{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "title": {
        "type": "string"
      },
      "salary": {
        "type": ["number", "null"],
        "minimum": 0
      },
      "equity": {
        "type": ["string", "null"],
        "pattern": "^(0(?:\\.\\d+)?|1(?:\\.0+)?)$"
      }
    },
    "additionalProperties": false
  }
  